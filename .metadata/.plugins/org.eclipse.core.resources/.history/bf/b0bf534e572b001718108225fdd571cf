package test;
/*将1-150不能被3整除的数输出出来
 * 
 * 
 */
public class Demo7 {
	
	public static void main(String[] args) {
		
		//循环1到150数然后把符合条件的数输出出来
		for(int i=1;i<=150;i++){
			
			//判断看看能否被整除
			if(i%3==0){
				
			}else{
				for(int j=1;j<=3;j++){
					if(j==3){
						System.out.println();
					}else{
						System.out.print(i+"\t");
				
					}
				}
			}
		}
	}

}
